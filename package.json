{
  "name": "jasmine",
  "description": "CLI for Jasmine, a simple JavaScript testing framework for browsers and Node",
  "homepage": "http://jasmine.github.io/",
  "keywords": [
    "test",
    "testing",
    "jasmine",
    "tdd",
    "bdd"
  ],
  "license": "MIT",
  "version": "5.11.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasmine/jasmine-npm"
  },
  "scripts": {
    "test": "node ./bin/jasmine.js",
    "posttest": "eslint \"bin/**/*.js\" \"lib/**/*.js\" \"spec/**/*.js\""
  },
  "exports": {
    ".": "./lib/jasmine.js",
    "./parallel": "./lib/parallel_runner.js"
  },
  "files": [
    "bin",
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "glob": "^10.2.2 || ^11.0.3",
    "jasmine-core": "github:jasmine/jasmine#6.0"
  },
  "bin": "./bin/jasmine.js",
  "main": "./lib/jasmine.js",
  "devDependencies": {
    "eslint": "^9.24.0"
  }
}
